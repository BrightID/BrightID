{
  "name": "BrightID",
  "version": "4.29.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appIdSuffix debug",
    "ios": "react-native run-ios  --simulator 'iPhone 14'",
    "ios:se": "react-native run-ios  --simulator 'iPhone SE (3rd generation)'",
    "start": "react-native start",
    "start:detox": "RN_SRC_EXT=e2e.js,e2e.ts react-native start",
    "build:detox:android": "detox build -c android.emu.debug",
    "build:detox:ios": "detox build -c ios.debug",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'  --assets-dest='./ios'",
    "test:android": "detox test -c android.emu.debug",
    "test:ios": "detox test -c ios.debug",
    "test": "jest",
    "lint": "eslint src",
    "postinstall": "npx jetify && npx patch-package",
    "test:watch": "npm test -- --watch",
    "updateSnap": "jest --updateSnapshot",
    "build:release": "cd android && ./gradlew assembleRelease && cd ..",
    "build:staging": "cd android && ./gradlew assembleStaging && cd ..",
    "mermaid": "find ./diagrams -type f -name *.mmd -exec mmdc -i {} \\;",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write"
  },
